<?xml version="1.0" encoding="UTF-8" ?>
<Shell
	x:Class="Alloro.DisasterEmergency.Mobile.AppShell"
	xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	xmlns:views="clr-namespace:Alloro.DisasterEmergency.Mobile.Views"
	xmlns:local="clr-namespace:Alloro.DisasterEmergency.Mobile">

	<ShellContent
		Shell.NavBarIsVisible="true"
		Title="Welcome"
		Icon="iconblank.png"
		ContentTemplate="{DataTemplate views:WelcomePage}"
		Route="WelcomePage" />
	<ShellContent
		Shell.NavBarIsVisible="true"
		Title="DisasterMap"
		Icon="iconmap.png"
		ContentTemplate="{DataTemplate views:DisasterMapPage}"
		Route="DisasterMapPage" />
	<ShellContent
		Shell.NavBarIsVisible="true"
		Title="ReportDisaster"
		Icon="iconblank.png"
		ContentTemplate="{DataTemplate views:ReportDisasterPage}"
		Route="ReportDisasterPage" />
</Shell>
